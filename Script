Frame.Visible = false
Main.Visible = true

local uis = game:GetService("UserInputService")
local debounce = 0

uis.InputBegan:Connect(function(key)
	if key.KeyCode == Enum.KeyCode.RightControl then
		if debounce == 0 then
			debounce = 2
			Main.Close:TweenSize(UDim2.new(1,0,-1,0),nil,nil,0.2)
			wait(0.2)
			Main:TweenSize(UDim2.new(0.250388622, 0, 0, 0),nil,nil,0.2)
			wait(0.2)
			Main.Visible = false
			debounce = 1
		elseif debounce == 1 then
			debounce = 2
			Main.Visible = true
			Main:TweenSize(UDim2.new(0.250388622, 0, 0.355328649, 0),nil,nil,0.2)
			wait(0.2)
			Main.Close:TweenSize(UDim2.new(1,0,0,0),nil,nil,0.2)
			wait(0.2)
			debounce = 0
		end
	end
end)

local mouse = game.Players.LocalPlayer:GetMouse()

for i, v in pairs(Main:GetChildren()) do
	if v:isA("ImageButton") then
		v.MouseEnter:Connect(function()
			v.ImageColor3 = Color3.fromRGB(42,42,42)
		end)
		v.MouseLeave:Connect(function()
			v.ImageColor3 = Color3.fromRGB(36,36,36)
		end)
	end
end

function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

counter = 0

