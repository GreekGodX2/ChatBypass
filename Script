local spam = 0

TextBox.Focused:Connect(function()
	spam = 1
end)
TextBox.FocusLost:Connect(function()
	spam = 0
end)

spawn(function()
	while wait(0.1)do
		TextLabel.TextColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.01
	end
end)

spawn(function()
	while true do
		wait(2.2)
		if TextBox.Text ~= nil and spam == 0 then
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(TextBox.Text,"All")
		else

		end
	end
end)

